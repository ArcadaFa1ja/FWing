generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// Kör 'npx prisma db push' i konsolen för att införa nya modeller eller ändringar till modeller i databasen.
model users {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  username String  @unique
}

model testModel{
  id  String @id @default(auto()) @map("_id") @db.ObjectId
}